  "homepage": "https://github.com/LitoMore/nofan#readme",
  "dependencies": {
    "boxen": "^2.0.0",
    "chalk": "^2.4.1",
    "chalk-pipe": "^2.0.0",
    "commander": "^2.17.1",
    "fanfou-sdk": "^3.0.1",
    "fanfou-streamer": "^0.2.1",
    "figlet": "^1.2.0",
    "gradient-string": "^1.1.0",
